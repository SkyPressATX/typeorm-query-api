{
  "name": "@skypress/typeorm-query-api",
  "version": "0.1.1",
  "description": "SkyPress TypeORM package to map URL Query to Find Options",
  "repository": {
    "url": "https://github.com/SkyPressATX/typeorm-query-api"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "build:sam": "rimraf .aws-sam && esbuild test/index.ts --bundle --platform=node --outdir=./test/dist --allow-overwrite && sam build",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "typeorm",
    "utility"
  ],
  "author": "BMO <bmo@skypress.io>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.195",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "esbuild": "^0.18.10",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.16",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "lodash": "^4.17.21",
    "typeorm": "^0.3.16"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s": "ts-jest"
    },
    "testEnvironment": "node"
  }
}
